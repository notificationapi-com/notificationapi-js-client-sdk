{
  "name": "notificationapi-js-client-sdk",
  "version": "5.0.0",
  "description": "NotificationAPI client-side library for JavaScript",
  "keywords": [
    "notificationapi",
    "js",
    "client"
  ],
  "author": "NotificationAPI (https://www.notificationapi.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notificationapi-com/notificationapi-js-client-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/notificationapi-com/notificationapi-js-client-sdk/issues"
  },
  "homepage": "https://www.notificationapi.com",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    },
    "./dist/styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "./lib/**/*",
    "./dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "test": "jest --collectCoverage",
    "prepack": "npm run build",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:dist && ./scripts/fixpackagetype.sh",
    "build:esm": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "build:cjs": "./node_modules/.bin/tsc -p ./tsconfig.cjs.json",
    "build:dist": "./node_modules/.bin/webpack && cp -R src/assets/* dist",
    "lint": "eslint . --ext .ts",
    "prettier-check": "prettier --config .prettierrc './**/*.ts' --check",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/html": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@types/javascript-time-ago": "^2.0.2",
    "@types/jest": "^26.0.14",
    "@types/jquery": "^3.5.5",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "cypress": "^10.4.0",
    "eslint": "^7.11.0",
    "jest": "^26.5.3",
    "jest-websocket-mock": "^2.2.0",
    "jquery": "^3.5.1",
    "mock-socket": "^9.0.3",
    "prettier": "^2.7.1",
    "sb": "^6.1.11",
    "ts-jest": "^26.4.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.0.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
